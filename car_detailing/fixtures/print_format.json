  "format_data": null,
  "html": "<div>\n    <h1 class=\"text-center\">Tax Invoice</h1>\n</div>\n\n<div class=\"text-left\" style=\"margin-top:15px;\">\n    <strong>{{doc.company}}</strong><br>\n    {% set address = frappe.get_all('Address',filters={'name':doc.company_address}, fields=['address_line1','address_line1','city','country'])%}\n    {% if address%}\n        {% for ad in address%}\n            <strong>{% if ad.address_line1 %}{{ad.address_line1}}{% endif %}</strong><br>\n            <strong>{% if ad.address_line2 %}{{ad.address_line2}}<br>{% endif %}</strong>\n            <strong>{% if ad.city %}{{ad.city}}{% endif %}</strong><br>\n            <strong>{% if ad.country %}{{ad.country}}{% endif %}</strong><br>\n        {% endfor %}\n    {% endif %}    \n</div>\n\n<div class=\"text-left\" style=\"margin-top:15px;\">\n    <strong>Invoice No.:</strong>{{doc.name}}<br>\n    <strong>Dated:</strong>{{frappe.format_date(doc.posting_date)}}\n</div>\n\n<div class=\"text-left\" style=\"margin-top:15px;\">\n    <strong>Customer:</strong> {{doc.customer}}<br>\n    <strong>Customer Address:</strong> {% if doc.address_display %}{{doc.address_display}}{% endif %}\n</div>\n\n<div style=\"margin-top:15px;border-width:1px;border-color:black;border-style:solid;\">\n    <table class=\"tg\" width=\"100%\" >\n        <thead>\n          <tr>\n            <th class=\"tg-0pky\">Description</th>\n            <th class=\"tg-0pky\">Quantity</th>\n            <th class=\"tg-0pky\">Rate(BHD)</th>\n            <th class=\"tg-0pky\">Per</th>\n            <th class=\"tg-0pky\">Amount(BHD)</th>\n            <th class=\"tg-0pky\">Discount</th>\n            <th class=\"tg-0pky\">Taxable Value</th>\n            <th class=\"tg-0pky\">Vat Rate</th>\n            <th class=\"tg-0pky\">Vat Amount<br>(BHD)</th>\n          </tr>\n        </thead>\n        <tbody>\n            {% for item in doc.items %}\n          <tr>\n            <td class=\"tg-0lax\">{{item.description}}</td>\n            <td class=\"tg-0lax\">{{item.qty}} {{item.uom}}</td>\n            <td class=\"tg-0lax\" style=\"white-space:nowrap;\">{{item.rate}}</td>\n            <td class=\"tg-0lax\">{{item.uom}}</td>\n            <td class=\"tg-0lax\">{{item.amount}}</td>\n            <td class=\"tg-0lax\">{% if discount_percentage %}{{item.discount_percentage}} {% endif %}</td>\n            <td class=\"tg-0lax\">{{item.amount}}</td>\n            <td class=\"tg-0lax\"></td>\n            <td class=\"tg-0lax\"></td>\n          </tr>\n          {% endfor %}\n          <tr>\n            <th class=\"tg-0lax\">Total</th>\n            <th class=\"tg-0lax\">{{doc.total_qty}}</th>\n            <th class=\"tg-0lax\"></th>\n            <th class=\"tg-0lax\"></th>\n            <th class=\"tg-0lax\">{{doc.total}}</th>\n            <th class=\"tg-0lax\"></th>\n            <th class=\"tg-0lax\"></th>\n            <th class=\"tg-0lax\"></th>\n            <th class=\"tg-0lax\"></th>\n          </tr>\n        </tbody>\n    </table>\n</div>\n\n<div class=\"text-right\" style=\"margin-top:15px;\">\n    <strong>For {{doc.company}}</strong><br>\n    <strong>Authorised Signature</strong>\n</div>",
