lass=\"tg-0lax\">\n                {% if item.checkbox == 1%}\n                    <input type=\"checkbox\" id=\"vehicle3\" name=\"vehicle3\" value=\"Boat\" checked>\n                {% else %}\n                    <input type=\"checkbox\" id=\"vehicle3\" name=\"vehicle3\" value=\"Boat\">\n                {% endif %}\n            </td>\n          </tr>\n        {% endif %}\n        {% endfor %}  \n        </tbody>\n    </table>\n</div>\n\n<h2 style=\"margin-top:15px!important;margin-left:20px;text-underline-offset: 5px;text-decoration: underline;\"><b>Outsourcing Report:-</b></h2>\n<div style=\"border-bottom:1px solid black;padding-bottom:50px;\">\n    <p style=\"margin-left:20px;\">{{doc.description}}</p>\n</div>\n\n<h2 style=\"margin-top:15px!important;margin-left:20px;text-underline-offset: 5px;text-decoration: underline;\"><b>Amounts:-</b></h2>\n<div class=\"row\" style=\"border-bottom:1px solid black;padding-bottom:20px;\">\n    <div class=\"col-xs-6\" style=\"margin-top:10px;\">\n        <p style=\"margin-left:20px;\">Packages Amount:<b>{{frappe.format(doc.packages_amounts,{'fieldtype':'Currency'})}}</b></p>\n        <p style=\"margin-left:20px;\">Outsourcing Amount:<b>{{frappe.format(doc.outsourcing_amount,{'fieldtype':'Currency'})}}</b></p>\n        <p style=\"margin-left:20px;\">Discount Amount:<b>{{frappe.format(doc.discount_amount,{'fieldtype':'Currency'})}}</b></p>\n    </div>\n    <div class=\"col-xs-6\" style=\"margin-top:10px;\">\n        <p style=\"margin-left:20px;\">Total Amount:<b>{{frappe.format(doc.total_amount,{'fieldtype':'Currency'})}}</b></p>\n        <p style=\"margin-left:20px;\">VAT Amount:<b>{{frappe.format(doc.vat_amount,{'fieldtype':'Currency'})}}</b></p>\n        <p style=\"margin-left:20px;\">Grand Total:<b>{{frappe.format(doc.grand_total,{'fieldtype':'Currency'})}}</b></p>\n    </div>\n</div>\n\n<h2 style=\"margin-top:15px!important;margin-left:20px;text-underline-offset: 5px;text-decoration: underline;\"><b>Customer Acknowledgment: