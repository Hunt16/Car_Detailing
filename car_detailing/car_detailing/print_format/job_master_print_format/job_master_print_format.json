ass=\"col-xs-6\" style=\"margin-top:10px;\">\n        <table width=\"100%\">\n            <tr>\n                <th>Date Of Receive:</th>\n                <td>{{frappe.format(doc.date_of_receive,{'fieldtype':'Date'})}}</td>\n            </tr>\n            <tr>\n                <th>Delivery Date:</th>\n                <td>{{frappe.format(doc.delivery_date,{'fieldtype':'Date'})}}</td>\n            </tr>\n        </table>\n    </div>\n    \n</div>\n\n<h2 style=\"margin-top:10px!important;margin-left:20px;text-decoration: underline;text-underline-offset: 5px;\"><b>Vehicle Information:-</b></h2>\n<div class=\"header\" style=\"border-bottom:1px solid black;padding-bottom:80px;\">\n    <div class=\"col-xs-6\" style=\"margin-top:10px;\">\n        <table width=\"100%\">\n            <tr>\n                <th>Kind Of Car/Vehicle:</th>\n                <td>{{doc.kind_of_carvehicle}}</td>\n            </tr>\n            <tr>\n                <th>Year Of Make:</th>\n                <td>{{doc.year_of_make}}</td>\n            </tr>\n        </table>\n    </div>\n    <div class=\"col-xs-6\" style=\"margin-top:10px;\">\n        <table width=\"100%\">\n            <tr>\n                <th>Plate/Chassis No:</th>\n                <td>{{doc.platechassis_no}}</td>\n            </tr>\n            <tr>\n                <th>Vehicle Color:</th>\n                <td>{{doc.vehicle_color}}</td>\n            </tr>\n        </table>\n    </div>\n    \n</div>\n<h2 style=\"margin-top:15px!important;margin-left:20px;text-decoration: underline;text-underline-offset: 5px;\"><b>Car Type:-</b></h2>\n<div class=\"row\" style=\"border-bottom:1px solid black;padding-bottom:20px;\">\n    <div class=\"col-xs-4\">\n        <form action=\"/action_page.php\" style=\"margin-left:20px;margin-top:10px;\">\n    \n                <div>\n                  {%if doc.convertible == 1%}\n                  <input type=\"checkbox\" id=\"vehicle1\" name=\"vehicle1\" value=\"Bike\" checked>\n                  {% else %}\n                  <input type=\"checkbox\" id=\"vehicle1\" name=\"vehicle1\" value=\"Bike\">\n                  {% endif %}\n                  <label for=\"vehicle1\"> Convertible</label><br>\n                  <img src=\"https://mrsh.tncbahrain.com/files/convertible-car%20(1)(1).png\">\n                </div>    \n                 <div>\n                     {%if doc.coupe == 1%}\n                     <input type=\"checkbox\" id=\"vehicle2\" name=\"vehicle2\" value=\"Car\" checked>\n                     {% else %}\n                      <input type=\"checkbox\" id=\"vehicle2\" name=\"vehicle2\" value=\"Car\">\n                      {% endif %}\n                      <label for=\"vehicle2\"> Coupe</label><br>\n                      <img src=\"https://mrsh.tncbahrain.com/files/coupe(1).jpg\">  \n                  </div>\n                <div>\n                    {%if doc.hatchback == 1%}\n                    <input type=\"checkbox\" id=\"vehicle3\" name=\"vehicle3\" value=\"Boat\" checked>\n                    {% else %}\n                    <input type=\"checkbox\" id=\"vehicle3\" name=\"vehicle3\" value=\"Boat\">\n                    {% endif %}\n                    <label for=\"vehicle3\"> Hatchback</label><br>\n                    <img src=\"https://mrsh.tncbahrain.com/files/hatchback-car(1).jpg\"> \n                </div>\n            \n            </form>\n    </div>\n    <div class=\"col-xs-4\">\n        <form action=\"/action_page.php\" style=\"margin-left:20px;margin-top:10px;\">\n                <div>\n                      {%if doc.sedan == 1%}\n                      <input type=\"checkbox\" id=\"vehicle1\" name=\"vehicle1\" value=\"Bike\" checked>\n                      {%else%}\n                      <input type=\"checkbox\" id=\"vehicle1\" name=\"vehicle1\" value=\"Bike\">\n                      {% endif %}\n                      <label for=\"vehicle1\"> Sedan</label><br>\n                      <img src=\"https://mrsh.tncbahrain.com/files/8526-8466dd65.p